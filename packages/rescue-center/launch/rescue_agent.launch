<launch>
    <arg name="botID"/>
    <group ns="$(arg botID)">
        <node pkg="rescue-center" type="rescue_agent_node.py" name="rescue_agent_$(arg botID)" output="screen">
          <param name="~distressed_veh" type="string" value="autobot$(arg botID)" />
        </node>
    </group>
</launch>   
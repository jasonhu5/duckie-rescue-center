          <param name="~trigger_rescue" type="bool" value="false"/>
        </node>
        
        <node pkg="rescue_center" type="rescue_manager_node.py" name="rescue_manager_27" output="screen">
          <param name="~distressed_veh" type="string" value="autobot27" />
        </node>
    </group>
</launch>

<launch>
    <arg name="veh" default="$(env VEHICLE_NAME)"/>
    <group ns="$(arg veh)">
        <node pkg="rescue-center" type="lane_recovery_node.py" name="lane_recovery_node" output="screen"/>
        <node pkg="rescue-center" type="rescue_trigger_node.py" name="rescue_trigger_node" output="screen">
          <param name="~trigger_rescue" type="bool" value="false"/>
        </node>
    </group>


</launch>
